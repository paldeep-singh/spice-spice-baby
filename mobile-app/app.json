{
  "expo": {
    "name": "Spice Spice Baby",
    "slug": "spice-spice-baby",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#22262E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.spicespicebaby"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#22262E"
      },
      "package": "com.anonymous.spicespicebaby"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
        }
      ]
    ]
  }
}